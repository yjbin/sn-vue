<script>
import { doCreate, getDicTab } from "@/utils/config";
import xianModal from "./xianModal";
import shiModal from "./shiModal";
import shengModal from "./shengModal";
import xiangModal from "./xiangModal";
import { xmmsChange } from "@/api/projectOutline";
export default {
    components: {
        xianModal,
        shiModal,
        shengModal,
        xiangModal
    },
    data() {
        return {
            editModalToggle: false,
            editTitleText: "",
            ndoptions: [""],
            cylxoptions: [""],
            xzqhoptions: [],
            bmbmoptions: [],
            xianModal: false,
            xiangModal: false,
            shiModal: false,
            shengModal: false,
            xianTit: "",
            xiangTit: "",
            shiTit: "",
            shengTit: "",
            editForm: {
                xmmc: "",
                nd: "",
                xmbh: "",
                xmlb: "",
                ssdw: "",
                fzr: "",
                kssj: "",
                jssj: "",
                xmze: "",
                zyZj: "",
                cylx:"",
                shengZj: "",
                shiZj: "",
                xianZj: "",
                xiangZj: "",
                xzqh: this.$store.state.user.user.uUser.xzqh,
                bmbm: this.$store.state.user.user.uUser.bmbm,
                // xzqh1: getDicTab(
                //     "xzqh",
                //     this.$store.state.user.user.uUser.xzqh
                // ),
                // bmbm1: getDicTab(
                //     "bmbm",
                //     this.$store.state.user.user.uUser.xzqh +
                //         "-" +
                //         this.$store.state.user.user.uUser.bmbm
                // ),
                lrr: this.$store.state.user.user.uUser.username,
                lrsj: "",
                xmdz: "",
                xmGps: "",
                jsnr: "",
                shengFgks: "",
                shiFgks: "",
                xianFgks: "",
                xiangFgks: ""
            }
        };
    },
    props: {
        editModal: Boolean,
        editTitle: String,
        editObj: {
            default: () => {}
        }
    },
    watch: {
        editModal(val) {
            this.editModalToggle = val;
        },
        editTitle(val) {
            this.editTitleText = val;
        },
        editObj(val) {
            for (let attr in val) {
                this.editForm[attr] = val[attr];
            }
        }
    },
    methods: {
        xianToggle(val) {
            this.xianModal = val;
        },
        xiangToggle(val) {
            this.xiangModal = val;
        },
        shiToggle(val) {
            this.shiModal = val;
        },
        shengToggle(val) {
            this.shengModal = val;
        },
        shengShow() {
            this.shengModal = true;
            this.shengTit = "省级分管处室";
        },
        shiShow() {
            this.shiModal = true;
            this.shiTit = "市级分管处室";
        },
        xianShow() {
            this.xianModal = true;
            this.xianTit = "县级分管处室";
        },
        xiangShow() {
            this.xiangModal = true;
            this.xiangTit = "县级分管处室";
        },
        shengFgks(val) {
            this.editForm.shengFgks = val;
        },
        shiFgks(val) {
            this.editForm.shiFgks = val;
        },
        xianFgks(val) {
            this.editForm.xianFgks = val;
        },
        xiangFgks(val) {
            this.editForm.xiangFgks = val;
        },
        btn_cancel() {
            this.editModalToggle = false;
            this.$emit("editToggle", this.editModalToggle);
        },
        userClose() {
            this.btn_cancel();
        },
        userbtn_save() {
            let _this = this;
            xmmsChange(_this.editForm).then(res => {
                let data = res.data;
                if (data.success) {
                    _this.btn_cancel();

                    this.$message({
                        message: data.msg,
                        type: "success"
                    });
                    let obj = {
                        pageNo: this.pageNo,
                        pageSize: this.pageSize
                    };
                    this.$emit("getList", obj);
                }
            });
        },
        getNowDate() {
            let d = new Date();
            this.editForm.lrsj =
                d.getFullYear() +
                "-" +
                (d.getMonth() + 1 < 10
                    ? "0" + (d.getMonth() + 1)
                    : d.getMonth() + 1) +
                "-" +
                (d.getDate() < 10 ? "0" + d.getDate() : d.getDate());
        }
    },
    mounted() {
        this.ndoptions = doCreate("nd");
        this.cylxoptions = doCreate("cylb");
        this.xzqhoptions = doCreate("xzqh");
        this.bmbmoptions = doCreate("bmbm");
    }
};
</script>
